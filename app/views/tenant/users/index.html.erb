<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tenant_users.each do |tenant_user| %>
      <tr>
        <td><%= tenant_user.email %></td>
        <td><%= link_to 'Show', tenant_user_path(id: tenant_user.id) %></td>
        <td><%#= link_to 'Edit', edit_tenant_user_path(id: tenant_user.id) %></td>
        <td><%#= link_to 'Destroy', tenant_user_path(id: tenant_user.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

